<h1>Lista Clientes</h1>

<table>
  <thead>
    <tr>
      <th>Cedula</th>
      <th>Nombre</th>
      <th>Fch ncto</th>
      <th>Mail</th>
      <th>Genero</th>
      <th>Celular</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clientes.each do |cliente| %>
      <tr>
        <td><%= cliente.cedula %></td>
        <td><%= cliente.nombre %></td>
        <td><%= cliente.fch_ncto %></td>
        <td><%= cliente.mail %></td>
        <td><%= cliente.genero %></td>
        <td><%= cliente.celular %></td>
        <td><%= link_to 'Ver', cliente %></td>
        <td><%= link_to 'Editar', edit_cliente_path(cliente) %></td>
        <td><%= link_to 'Eliminar', cliente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Cliente', new_cliente_path %>
